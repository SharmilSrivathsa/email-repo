
echo "# email-repo" >> README.md   # Create a README file (optional but good practice)
git init                           # Initialize a new Git repository
git add README.md                  # Stage the README file
git commit -m "first commit"       # Commit the README file to the repository
git branch -M main                 # Rename the default branch to 'main'
git remote add origin https://github.com/SharmilSrivathsa/email-repo.git  # Add the remote GitHub repository URL
git push -u origin main            # Push the initial commit to GitHub
